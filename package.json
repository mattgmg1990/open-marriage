{
    "name": "wilson-garnes.com",
    "description": "The open source website for Katie Wilson's and Matt Garnes's wedding.",
    "homepage": "http://wilson-garnes.com/",
    "version": "1.8.0",
    "private": true,
    "author": "Eric Ferraiuolo <eferraiuolo@gmail.com> (http://ericf.me/)",
    "license": "MIT",
    "directories": {
        "lib": "./lib",
        "middleware": "./middleware",
        "public": "./public",
        "routes": "./routes",
        "scripts": "./scripts",
        "views": "./views"
    },
    "main": "server",
    "engines": {
        "node": "12.3.1",
        "npm": ">=1.1.2"
    },
    "dependencies": {
        "async": "0.2.x",
        "bower": "1.8.8",
        "combohandler": "0.2.x",
        "csv": "0.3.x",
        "db-migrate": "0.4.x",
        "deep-freeze": "latest",
        "express": "3.2.x",
        "express-slash": "latest",
        "express-state": "latest",
        "express3-handlebars": "latest",
        "handlebars": "1.0.x",
        "pg": "7.7.1",
        "request": "2.25.0"
    },
    "scripts": {
        "install": "./node_modules/bower/bin/bower install",
        "migrate-up": "./node_modules/db-migrate/bin/db-migrate up -e default --config config/migrations.json"
    }
}
